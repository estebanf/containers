<?xml version="1.0" encoding="utf-8"?>

<urlrewrite>

    <rule match-type="regex">
       <from>^/deployment/?$</from>
       <to qsappend="true" type="redirect">/everteam/ode/deployment</to>
    </rule>
	
    <rule match-type="regex">
       <from>(/gi/.*)$</from>
       <to context="everteam" qsappend="true" type="forward">$1</to>
    </rule>
    
    <rule match-type="regex">
       <from>^/axis2-admin/?$</from>
       <to type="redirect">/ode/axis2-admin</to>
    </rule>

    <rule match-type="regex">
       <from>^(/axis2-admin/.*)$</from>
       <to context="everteam" qsappend="true" type="forward">/ode$1?</to>
    </rule>

    <rule match-type="regex">
       <from>^/axis2/?$</from>
       <to qsappend="true" type="redirect">/everteam/axis2-web/index.jsp</to>
    </rule>

    <rule match-type="regex">
       <from>^/ode/?$</from>
       <to qsappend="true" type="redirect">/everteam/axis2-web/index.jsp</to>
    </rule>

    <rule match-type="regex">
       <from>^/ui-fw/script/empty.jsp$</from>
       <to qsappend="true" type="redirect">/everteam/workflow/script/empty.jsp</to>
    </rule>

</urlrewrite>