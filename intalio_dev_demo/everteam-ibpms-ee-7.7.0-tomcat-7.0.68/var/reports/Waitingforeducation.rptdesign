                    <property name="name">Waiting</property>
                    <text-property name="displayName">Waiting period</text-property>
                    <expression name="expression">function calculateWaiting(waiting){
  var d1 = new Date(waiting);
  var t1 = d1.getTime();
  var d2 = new Date();
  var t2 = d2.getTime();
  var dif = t2-t1;
  var mins = 1000*60;
  var hours = mins*60;
  var days = hours*24;
  var rmins = parseInt(dif/mins);
  var rhours = parseInt(dif/hours);
  var rdays = parseInt(dif/days);
  if(rdays > 0){
    return rdays + " days";
  }
  else{
    if(rhours > 0) {
      return rhours + " hours";
    }
    else{
      return rmins + " mins";
    }
  }
}
calculateWaiting(row["educationstartdate"]);</expression>
