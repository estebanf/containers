<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <datasource>com.intalio.bpms.jndi.bpmsdb</datasource>
    <connector>
        <maxResponseBytes value="1000000" />
        <globalFeedQueryTimeout value="300" />
    </connector>  
    <databases>
        <!-- Derby 10 -->
        <database type="DER10">
            <ErrorRanges>
            <range lower="-10000" upper="-100"/>
            </ErrorRanges>
        </database>
        <!-- MySQL 5 -->
        <database type="MySql5.1">
            <ErrorRanges>
            <range lower="-10000" upper="-100"/>
            </ErrorRanges>
        </database>
</databases>
</configuration>