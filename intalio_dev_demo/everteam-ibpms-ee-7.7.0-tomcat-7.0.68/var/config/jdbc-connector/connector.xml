<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <connector>
        <maxResponseBytes value="1000000" />
    </connector>
    <databases>
        <!-- Derby 10 -->
        <database type="DER10">
            <ErrorRanges>
            <range lower="-10000" upper="-100"/>
